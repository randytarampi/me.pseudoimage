{
	"extension": [
		".jsx"
	],
	"exclude": [
		"node_modules/**",
		"bin/**",
		"coverage/**",
		".nyc_output/**",
		"test/**",
		"*.js"
	],
	"reporter": [
		"text-summary",
		"cobertura",
		"lcov"
	],
	"cache": true,
	"all": true,
	"check-coverage": true,
	"sourceMap": true,
	"instrument": true,
	"watermarks": {
		"lines": [
			70,
			90
		],
		"functions": [
			70,
			90
		],
		"branches": [
			70,
			90
		],
		"statements": [
			70,
			90
		]
	}
}
